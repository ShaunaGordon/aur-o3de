_engver=21.11.2

post_install() {

	# Run script to install python version local to engine
	"/opt/O3DE/${_engver}/python/get_python.sh"
	
	# Change ownership to user
    if [ ! "$(logname 2>&1 >/dev/null)" ]; then
        _user=$(logname)
        _group=$(id -g -n ${_user})
    else
        _user=root
        _group=root
    fi
    chown -R ${_user}:${_group} /opt/O3DE/${_engver}

}
